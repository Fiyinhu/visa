[1;58r[57;1H## Read drop-in files from /etc/sudoers.d (the # here does not mean a comment)[?25h[?25l[1;57r[57;1H
[1;58r[57;1H#includedir /etc/sudoers.d[?25h[56;1H[55;1H[54;1H[53;1H[52;1H[51;1H[50;1H[49;1H[48;1H[47;1H[46;1H[45;1H[44;1H[43;1H[42;1H[41;1H[39;1H[38;1H[37;1H[36;1H[35;1H[34;1H[33;1H[32;1H[31;1H[30;1H[29;1H[28;1H[27;1H[26;1H[25;1H[24;1H[23;1H[22;1H[21;1H[20;1H[19;1H[18;1H[17;1H[16;1H[15;1H[14;1H[13;1H[12;1H[11;1H[10;1H[9;1H[8;1H[7;1H[6;1H[5;1H[4;1H[3;1H[2;1H[1;1H[?25l[1;57r[1;1H[L[1;58r[1;1HDefaults    match_group_by_gid[?25h[?25l[1;57r[1;1H[L[1;58r[1;1HDefaults    always_set_home[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H#[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# env_reset is disabled or HOME is present in the env_keep list.[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# this option is only effective for configurations where either[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# is already set when the the env_reset option is enabled, so[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# use it when searching for configuration files. Note that HOME[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# Preserving HOME has security implications since many programs[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H#[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1HDefaults   !visiblepw[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H#[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# Refuse to run if unable to disable echo on the tty.[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H#[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# Defaults specification[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# Cmnd_Alias DRIVERS = /sbin/modprobe[57;1H[1m[34m@                                                                                   [1;1H[?25h[?25l[1;57r[m[1;1H[L[1;58r[1;1H## Drivers[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# Cmnd_Alias PROCESSES = /bin/nice, /bin/kill, /usr/bin/kill, /usr/bin/killall[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Processes[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# Cmnd_Alias DELEGATING = /usr/sbin/visudo, /bin/chown, /bin/chmod, /bin/chgrp[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Delegating permissions[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[2L[1;58r[1;1H# Cmnd_Alias STORAGE = /sbin/fdisk, /sbin/sfdisk, /sbin/parted, /sbin/partprobe, /bii[2;1Hn/mount, /bin/umount[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Storage[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# Cmnd_Alias LOCATE = /usr/bin/updatedb[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Updating the locate database[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[3L[1;58r[1;1H# Cmnd_Alias SERVICES = /sbin/service, /sbin/chkconfig, /usr/bin/systemctl start, /uu[2;1Hsr/bin/systemctl stop, /usr/bin/systemctl reload, /usr/bin/systemctl restart, /usr/bb[3;1Hin/systemctl status, /usr/bin/systemctl enable, /usr/bin/systemctl disable[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Services[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# Cmnd_Alias SOFTWARE = /bin/rpm, /usr/bin/up2date, /usr/bin/yum[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Installation and management of software[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[3L[1;58r[1;1H# Cmnd_Alias NETWORKING = /sbin/route, /sbin/ifconfig, /bin/ping, /sbin/dhclient, /uu[2;1Hsr/bin/net, /sbin/iptables, /usr/bin/rfcomm, /usr/bin/wvdial, /sbin/iwconfig, /sbin//[3;1Hmii-tool[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Networking[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## These are groups of related commands...[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Command Aliases[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# User_Alias ADMINS = jsmith, mikem[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## rather than USERALIAS[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## (ie, from files, LDAP, NIS, etc) in this file - just use %groupname[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## These aren't often necessary, as you can use regular groups[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## User Aliases[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# Host_Alias     MAILSERVERS = smtp, smtp2[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H# Host_Alias     FILESERVERS = fs1, fs2[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## wildcards for entire domains) or IP addresses instead.[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Groups of machines. You may prefer to use hostnames (perhaps using[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Host Aliases[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## This file must be edited with the 'visudo' command.[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H##[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## users or groups.[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## of related commands, which can then be delegated out to particular[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Examples are provided at the bottom of the file for collections[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H##[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## the root user, without needing the root password.[?25h[?25l[1;57r[1;1H[L[1;58r[1;1H## Sudoers allows particular users to run various commands as[?25h


[3;1H[2;1H[1;1H























































[?25l[1;57r[57;1H
[1;58r[57;1H# Refuse to run if unable to disable echo on the tty.[?25h[?25l[1;57r[57;1H
[1;58r[57;1H#[?25h[?25l[1;57r[57;1H
[1;58r[57;1HDefaults   !visiblepw[?25h[?25l[1;57r[57;1H
[1;58r[57;1H[?25h[?25l[1;57r[57;1H
[1;58r[57;1H#[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# Preserving HOME has security implications since many programs[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# use it when searching for configuration files. Note that HOME[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# is already set when the the env_reset option is enabled, so[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# this option is only effective for configurations where either[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# env_reset is disabled or HOME is present in the env_keep list.[?25h[?25l[1;57r[57;1H
[1;58r[57;1H#[?25h[?25l[1;57r[57;1H
[1;58r[57;1HDefaults    always_set_home[?25h[?25l[1;57r[57;1H
[1;58r[57;1HDefaults    match_group_by_gid[?25h[?25l[1;57r[57;1H
[1;58r[57;1H[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# Prior to version 1.8.15, groups listed in sudoers that were not[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# found in the system group database were passed to the group[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# plugin, if any. Starting with 1.8.15, only groups of the form[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# %:group are resolved via the group plugin by default.[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# We enable always_query_group_plugin to restore old behavior.[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# Disable this option for new behavior.[?25h[?25l[1;57r[57;1H
[1;58r[57;1HDefaults    always_query_group_plugin[?25h[?25l[1;57r[57;1H
[1;58r[57;1H[?25h[?25l[1;57r[57;1H
[1;58r[57;1HDefaults    env_reset[?25h[?25l[1;57r[57;1H
[1;58r[57;1HDefaults    env_keep =  "COLORS DISPLAY HOSTNAME HISTSIZE KDEDIR LS_COLORS"[?25h[?25l[1;57r[57;1H
[1;58r[57;1HDefaults    env_keep += "MAIL PS1 PS2 QTDIR USERNAME LANG LC_ADDRESS LC_CTYPE"[?25h[?25l[1;57r[57;1H
[1;58r[57;1HDefaults    env_keep += "LC_COLLATE LC_IDENTIFICATION LC_MEASUREMENT LC_MESSAGES"[?25h[?25l[1;57r[1;1H[3M[1;58r[55;1HDefaults    env_keep += "LC_MONETARY LC_NAME LC_NUMERIC LC_PAPER LC_TELEPHONE"
Defaults    env_keep += "LC_TIME LC_ALL LANGUAGE LINGUAS _XKB_CHARSET XAUTHORITY"[55;1H[?25h

[?25l[1;57r[57;1H
[1;58r[57;1H#[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# Adding HOME to env_keep may enable a user to run unrestricted[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# commands via sudo.[?25h[?25l[1;57r[57;1H
[1;58r[57;1H#[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# Defaults   env_keep += "HOME"[?25h[?25l[1;57r[1;1H[3M[1;58r[56;1HDefaults    secure_path = /sbin:/bin:/usr/sbin:/usr/bin[55;1H[?25h

[?25l[1;57r[57;1H
[1;58r[57;1H## Next comes the main part: which users can run what software on[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## which machines (the sudoers file can be shared between multiple[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## systems).[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## Syntax:[?25h[?25l[1;57r[57;1H
[1;58r[57;1H##[?25h[?25l[1;57r[1;1H[2M[1;58r[56;1H##      user    MACHINE=COMMANDS
##[56;1H[?25h
[?25l[1;57r[57;1H
[1;58r[57;1H## The COMMANDS section may have other options added to it.[?25h[?25l[1;57r[57;1H
[1;58r[57;1H##[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## Allow root to run any commands anywhere[?25h[?25l[1;57r[57;1H
[1;58r[57;1Hroot    ALL=(ALL)[7CALL[?25h[?25l[1;57r[57;1H
[1;58r[57;1H[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## Allows members of the 'sys' group to run networking, software,[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## service management apps and more.[?25h[?25l[1;57r[1;1H[2M[1;58r[56;1H# %sys ALL = NETWORKING, SOFTWARE, SERVICES, STORAGE, DELEGATING, PROCESSES, LOCATE,,[57;1H DRIVERS[56;1H[?25h[?25l[1;57r[57;1H
[1;58r[57;1H[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## Allows people in group wheel to run all commands[?25h[?25l[1;57r[57;1H
[1;58r[57;1H%wheel  ALL=(ALL)[7CALL[?25h[?25l[1;57r[57;1H
[1;58r[57;1H[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## Same thing without a password[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# %wheel[8CALL=(ALL)[7CNOPASSWD: ALL[?25h[?25l[1;57r[57;1H
[1;58r[57;4HFiyin[8CALL=(ALL)[7CNOPASSWD: ALL[?25h[?25l[1;57r[57;1H
[1;58r[57;1H[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## Allows members of the users group to mount and unmount the[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## cdrom as root[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# %users  ALL=/sbin/mount /mnt/cdrom, /sbin/umount /mnt/cdrom[?25h[?25l[1;57r[57;1H
[1;58r[57;1H[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## Allows members of the users group to shutdown this system[?25h[?25l[1;57r[57;1H
[1;58r[57;1H# %users  localhost=/sbin/shutdown -h now[?25h[?25l[1;57r[57;1H
[1;58r[57;1H[?25h[?25l[1;57r[57;1H
[1;58r[57;1H## Read drop-in files from /etc/sudoers.d (the # here does not mean a comment)[?25h[?25l[1;57r[57;1H
[1;58r[57;1H#includedir /etc/sudoers.d[?25h[56;1H[55;1H[54;1H[53;1H[52;1H[51;1H[50;1H[49;1H[48;1H[47;1H[46;1H[45;1H











